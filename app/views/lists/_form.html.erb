<%= form_for(@list, :method=>:post,:remote=>true) do |f| %>
<%= f.hidden_field :user_id,:value=>session[:user_id] %>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
